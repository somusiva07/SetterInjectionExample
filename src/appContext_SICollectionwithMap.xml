<?xml version="1.0" encoding="UTF-8"?>
<beans
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


<bean id="employeeMap" class="learn.spring.EmployeeWithCollectionMap">
	<property name="employeeNumber" value="101"></property>
	<property name="employeeName" value="Somasundaram"></property>
	<property name="skillSet">
		<map>
			<entry key="Java" value="Java, J2EE, Spring"></entry>
			<entry key="Python" value="Python"></entry>
			<entry key="Cloud" value="AWS, Azure, Pivotal"></entry>
		</map>
	</property>
</bean>

</beans>